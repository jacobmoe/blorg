{{define "sidebar"}}

<img class="org-unicorn" src="images/org-mode-unicorn-logo.png" />
<ul class="nav nav-pills nav-stacked sidebar">
  {{range $index, $title := sectionTitles}}
    <li id="{{$index | plus1}}">
      <a href="#/pages/{{$index | plus1}}/{{$title | slugify}}">
        {{$title}}
      </a>
    </li>
  {{end}}
</ul> 

{{end}}
